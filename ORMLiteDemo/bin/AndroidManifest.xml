<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ormlitedemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.qburst.ormlitedemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.qburst.ormlitedemo.DataBaseViewer"
            android:label="@string/title_activity_data_base_viewer" >
        </activity>
        <activity
            android:name="com.qburst.ormlitedemo.AddToDataBase"
            android:label="@string/title_activity_add_to_data_base" >
        </activity>
        <activity
            android:name="com.qburst.ormlitedemo.DeleteFromDatabase"
            android:label="@string/title_activity_delete_from_database" >
        </activity>
        <activity
            android:name="com.qburst.ormlitedemo.SearchDatabase"
            android:label="@string/title_activity_search_database" >
        </activity>
    </application>

</manifest>
